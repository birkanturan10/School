@model IEnumerable<TypeLayer.Students>

@{
	Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Öğrenci Listesi</title>
	<!--Bootstrap Links-->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
	<!--LoginSheet.css-->
	<link href="~/stylesheets/loginsheet.css" rel="stylesheet" />
</head>
<body>
	<div style="padding:100px;">
		<div class="container" style="width:300px;">
			<table class="table">
				<thead>
					<tr>
						<th scope="col">@Html.DisplayNameFor(model=>model.StudentID)</th>
						<th scope="col">@Html.DisplayNameFor(model=>model.NameSurname)</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<th scope="row">@item.StudentID</th>
							<td>@item.NameSurname</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>
</body>
</html>
