@model IEnumerable<TypeLayer.Notes>

@{
	Layout = null;
}

<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Öğretmen Kontrol</title>
	<!--Bootstrap Links-->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
	<!--LoginSheet.css-->
	<link href="~/stylesheets/loginsheet.css" rel="stylesheet" />
</head>
<body>
	<div style="padding:30px;">
		<div class="container">
			<table class="table">
				<thead>
					<tr>
						<th scope="col">@Html.DisplayNameFor(model=>model.NoteID)</th>
						<th scope="col">@Html.DisplayNameFor(model=>model.LessonID)</th>
						<th scope="col">@Html.DisplayNameFor(model=>model.FirstExam)</th>
						<th scope="col">@Html.DisplayNameFor(model=>model.SecondExam)</th>
						<th scope="col">@Html.DisplayNameFor(model=>model.Project)</th>
						<th scope="col">@Html.DisplayNameFor(model=>model.AverageNote)</th>
					</tr>
				</thead>
				<tbody>
					@foreach (var item in Model)
					{
						<tr>
							<th scope="row">@Html.DisplayFor(ModelItem=>item.NoteID)</th>
							<td>@Html.DisplayFor(ModelItem=>item.LessonID)</td>
							<td>@Html.DisplayFor(ModelItem=>item.FirstExam)</td>
							<td>@Html.DisplayFor(ModelItem=>item.SecondExam)</td>
							<td>@Html.DisplayFor(ModelItem=>item.Project)</td>
							<td>@Html.DisplayFor(ModelItem=>item.AverageNote)</td>
						</tr>
					}
				</tbody>
			</table>
		</div>
	</div>
</body>
</html>
